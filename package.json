{
  "name": "adriel",
  "version": "1.0.0",
  "description": "Sistema de Marketing e Gest√£o - Adriel",
  "private": true,
  "scripts": {
    "dev": "node scripts/start-dev.js",
    "dev:simple": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "setup:local": "npm run install:all && npm run docker:dev && npm run db:migrate && npm run db:seed",
    "db:migrate": "cd backend && npm run prisma:migrate",
    "db:generate": "cd backend && npm run prisma:generate",
    "db:seed": "cd backend && npm run prisma:seed",
    "db:studio": "cd backend && npx prisma studio",
    "create:admin": "cd backend && npm run create:admin",
    "check": "node scripts/check-setup.js",
    "fix:env": "node scripts/fix-env.js"
  },
  "keywords": [
    "marketing",
    "multi-tenant",
    "react",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
